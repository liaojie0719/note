.indexPage {
  position: relative;
}
.flex-right {
  position: fixed;
  width:30px;
  height: 100%;
  top:89px;
  right:0;
  font-size:9px;
  color: #999;
  text-align: center;
  z-index: 999;
}
.each-letter {
  padding:1px 0;
}
.flex-content {
  z-index: 10;
}
.content-key {
  width: 100%;
  height: 40px;
  line-height: 40px;
  border-bottom: 1px solid #dbdbdb;
  padding-left:10px;
}
.border-bottom {
  overflow: hidden;
  height:50px;
  line-height: 50px;
  border-bottom:1px solid #dbdbdb;
  padding-left: 10px;
}
.imgurl {
  width:35px;
  height: 35px;
  float: left;
  transform: translateY(6px)
}
.logo-name {
  float: left;
  font-size: 14px;
  color: #666;
  margin-left: 10px;
}
.li {
  border-bottom:10px solid #edf0f5;
} 
.popCar {
  position: fixed;
  background: rgba(0,0,0,0.7);
  width: 80px;
  height: 80px;
  line-height: 80px;
  text-align: center;
  color: #fff;
  font-size: 40px;
  left:40%;
  top:40%;
  z-index: 100;
  border-radius: 10px;
}
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}
.my-search {
  background: #dbdbdb;
  border-bottom:none;
}
.li-search {
  background: #edf0f5;
}
.content-box {
  width:95%;
  margin:0 auto;
  background: #fff;
  text-align: center;
  font-size:12px;
  color: #666;
  height:30px;
  line-height: 30px; 
  border-radius: 5px;
}
.content-box image {
  transform: translateY(2px)
}
.myContent {
  padding-left: 0px;
  padding-top: 4px;
  padding-bottom: 4px;
  height: 30px;
}