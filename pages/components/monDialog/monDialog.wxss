.dialog-box {
  position: fixed;
  width: 100%;
  height: 100%;
  z-index: 999;
  overflow: hidden;
}
.dialog-content {
  width:25%;
  border-radius: 20rpx;
  float: left;
  color: #fff;
  text-align: center;
  padding: 10px 0;
  font-size: 14px;
}
.cancelButton {
  width:25px;
  height: 25px;
}
.cancel {
  display: flex;
  justify-content: space-between;
  padding:10px;
}
.now-month {
  height: 100%;
  background-color: rgba(255, 255, 255, 0.3);
}